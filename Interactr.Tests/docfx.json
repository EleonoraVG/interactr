{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "*.csproj"
          ],
          "cwd": ".",
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "Documentation_build/**"
          ]
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "Documentation/api/**.yml"
        ],
        "cwd": "obj"
      },
      {
        "files": [
          "Documentation/api/*.md",
          "Documentation/articles/**.md",
          "Documentation/toc.yml",
          "Documentation/*.md"
        ],
        "exclude": [
          "obj/**",
          "Documentation_build/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "Documentation/images/**"
        ],
        "exclude": [
          "obj/**",
          "Documentation_build/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "Documentation_build/**"
        ]
      }
    ],
    "dest": "Documentation_build",
    "template": [
      "default"
    ]
  }
}